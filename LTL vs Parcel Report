function repsplit() {
 
  SpreadsheetApp.openByUrl("https://docs.google.com/spreadsheets/d/1RQ4b4Rv4bJiYuHXP6u2uOUXBR3i2OIYlZlZ_wwGDD0I/edit#gid=1822529590")
  var name1 = "Josh Haskins"
  var name2 = "Joe Assencoa"
  var name3 = "Tim Donegan"
