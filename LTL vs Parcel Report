  var name8 = "Baker Leavitt"
  var name9 = "Matt White"
  
  var mag1 = SpreadsheetApp.getActiveSpreadsheet();
  var magtitle = mag1.getSheetByName("Magento Export").getRange("A1:Q1").getValues();
  
 
  
  
  
  var newsheet1 = SpreadsheetApp.getActiveSpreadsheet().insertSheet(name1);
  newsheet1.getRange("e4:u4").setValues(magtitle);
  newsheet1.getRange("a1").setValue(name1).setBackground("black");
  newsheet1.getRange("E5").setFormula("=filter('Magento Export'!A2:Q1715,'Magento Export'!a2:A1715=a1)");
  newsheet1.getRange("b2").setValue("LTL");
  newsheet1.getRange("c2").setValue("Parcel");  
  newsheet1.getRange("B3").setFormula("=sumif($p$2:$p,$b$2,$s$2:$s)").setNumberFormat("$0");
  newsheet1.getRange("C3").setFormula("=sumif($p$2:$p,$c$2,$s$2:$s)").setNumberFormat("$0");                                    
  newsheet1.getRange("B4").setFormula("=sumif($p$2:$p,$b$2,$o$2:$o)").setNumberFormat("0");
  newsheet1.getRange("C4").setFormula("=sumif($p$2:$p,$c$2,$o$2:$o)").setNumberFormat("0");
  SpreadsheetApp.getActiveSheet().getRange("A3").setValue("Total $");
  SpreadsheetApp.getActiveSheet().getRange("A4").setValue("Total Quantity");
  SpreadsheetApp.getActiveSheet().getRange("A1:D5").setHorizontalAlignment("Center");
  SpreadsheetApp.getActiveSheet().getRange("E:U").copyValuesToRange(newsheet1, 5, 21, 1, 2000);
  
 
                                      
                                      
